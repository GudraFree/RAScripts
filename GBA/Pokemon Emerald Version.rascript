function pointerBlock1() => tbyte(a_pointerBlock1)
function pointerBlock2() => tbyte(a_pointerBlock2) + 0x8000
function map() => word_be(pointerBlock1() + o_mapId)
function securityKey() => dword(pointerBlock2() + o_securityKey)
function money() => dword(pointerBlock1() + o_money)
function flag(f) => bit(f%8, pointerBlock1() + o_baseOffsetFlags + f/8)
function counter(offset) => word(pointerBlock1() + offset)
function myPokemonHeldItem() => word(a_myPokemonHeldItem)
function enemyPokemonId() => word(a_enemyPokemonId)
function enemyPokemonHeldItem() => word(a_enemyPokemonHeldItem)
function battleResult() => byte(a_battleResult)
function musicId() => word(a_musicId)
function trainerId() => word(a_trainerId)
function partyMonPersonality(i=1, size=4, offset=0) {
    address = a_partyMonStart + o_partyMonDatablock*(i-1) + o_partyMonPersonality + offset
    if(size==1) return byte(address)
    if(size==2) return word(address)
    return dword(address)
}
function partyMonTID(i=1, size=4, offset=0) {
    address = a_partyMonStart + o_partyMonDatablock*(i-1) + o_partyMonTID + offset
    if(size==1) return byte(address)
    if(size==2) return word(address)
    return dword(address)
}
function partyMonIsEgg(i=1) => bit2(a_partyMonStart + o_partyMonDatablock*(i-1) + o_partyMonMiscFlags)
function partyMonRibbons(struct, i=1) => dword(a_partyMonStart + o_partyMonDatablock*(i-1) + o_partyMonDataStructStart + o_partyMonDataStructDatablock*(struct-1) + o_partyMonRibbons)
function partyMonFriendship(struct, i=1) => byte(a_partyMonStart + o_partyMonDatablock*(i-1) + o_partyMonDataStructStart + o_partyMonDataStructDatablock*(struct-1) + o_partyMonFriendship)
function partyMonMoveId(struct, i=1, j=1) => word(a_partyMonStart + o_partyMonDatablock*(i-1) + o_partyMonDataStructStart + o_partyMonDataStructDatablock*(struct-1) + o_partyMonMoveId + (j-1)*2)
function slotsState() => byte(a_slotsState)
function slotsPayout() => word(a_slotsPayout)
function blendStateEnd() => byte(a_blendStateEnd)
function blendPlayers() => word(a_blendPlayers)
function pokenewsKind(i) => byte(pointerBlock1() + o_pokenewsStart + o_pokenewsDatablock*(i-1) + o_pokenewsKind)
function pokenewsState(i) => byte(pointerBlock1() + o_pokenewsStart + o_pokenewsDatablock*(i-1) + o_pokenewsState)
function playerTID() => dword(pointerBlock2() + o_playerTID)
function playerApprenticeId() => byte(pointerBlock2() + o_playerApprenticeId)
function playerApprenticeData() => byte(pointerBlock2() + o_playerApprenticeData)
function apprenticeId(i) => byte(pointerBlock2() + o_apprenticeStart + o_apprenticeDatablock*(i-1) + o_apprenticeId) & 0x1f 
function apprenticeTID(i) => dword(pointerBlock2() + o_apprenticeStart + o_apprenticeDatablock*(i-1) + o_apprenticeTID)
function apprenticeChecksum(i) => dword(pointerBlock2() + o_apprenticeStart + o_apprenticeDatablock*(i-1) + o_apprenticeChecksum)
function bfTrainer(i=0) => word(pointerBlock2() + o_bfTrainersStart + 2*i)
function roomDeco(i) => byte(pointerBlock1() + o_roomDecoStart + i-1)
function baseDeco(i) => byte(pointerBlock1() + o_baseDecoStart + i-1)
function decorId(i, type) {
    offset = 0
    // sums the slots of all previous types to get the start of the type passed
    if(type>0) for j in range(0, type-1) offset = offset + decorSlots[j]
    return byte(pointerBlock1() + o_decoInventoryStart + offset + i)
}
function battleChallengeStatus() => byte(pointerBlock2() + o_bfCurrentStatus)
function pcItem(i) => word(pointerBlock1() + o_pcItemStart + (i-1)*4)
function bagItem(i) => word(pointerBlock1() + o_bagItemStart + (i-1)*4)
function ingame() => byte(a_ingame)
function input(i) => bit(i%8, a_inputs + i/8)
function menu() => byte(a_menu)
function trainerHillMode() => byte(pointerBlock1() + o_trainerHill) & 0xc0 // leaves only the top 2 bits for the mode
function trainerHillEnd() => bit2(pointerBlock1() + o_trainerHill)
function contestWinnerPV() => dword(pointerBlock1() + o_contestWinner + o_contestWinnerPV)
function contestWinnerTID() => dword(pointerBlock1() + o_contestWinner + o_contestWinnerTID)
function contestWinnerType() => dword(pointerBlock1() + o_contestWinner + o_contestWinnerType)
function contestWinnerRank() => dword(pointerBlock1() + o_contestWinner + o_contestWinnerRank)
function gameStat(stat) => dword(pointerBlock1() + o_gameStatsStart + stat*4)
function cutRadio() => byte(a_cutRadio)
function dexFlag(i) => bit((i-1)%8, pointerBlock2() + o_dexCaughtStart + (i-1)/8)
function rematch(i) => byte(pointerBlock1() + o_rematchWally + i) // expects an index from rematches lookup

function isInGame() => ingame() == 1
function isPaused() => menu() == 1
function isInBattle() => musicId() == MUS_BATTLE
function pointerSanity(n=1) { 
    p = pointerBlock1()
    if(n==2) p = pointerBlock2()
    return repeated(20, p == prev(p)) && never(p != prev(p))
}
function flagTrigger(f, inverted=false, map=-1) {
    trigger = pointerSanity()
    if(map>-1) trigger = trigger && map()==map
    if(inverted) trigger = trigger && prev(flag(f)) > flag(f)
    else trigger = trigger && flag(f) > prev(flag(f))
    
    return trigger
}
function anyFlagTrigger(flags, inverted=false, map=-1) => any_of(flags, f => flagTrigger(f, inverted, map))
function counterTrigger(offset, target=1, measured=false) {
    trigger = pointerSanity()
    counter = counter(offset)
    if(measured) trigger = trigger && measured(counter==target)
    else trigger = trigger && counter==target
    trigger = trigger && prev(counter)==target-1
    
    return trigger
}
function collect(flags, measured=true, target=-1, maxIncrement=1) {
    if(target==-1) target = length(flags)
    trigger = pointerSanity()
    flagSum = sum_of(flags, f => flag(f))
    if(measured) trigger = trigger && measured(flagSum == target)
    else trigger = trigger && flagSum == target
    if(maxIncrement==1) trigger = trigger && prev(flagSum) == target-1
    else trigger = trigger && prev(flagSum) >= target-maxIncrement && prev(flagSum) < target
    
    return trigger
}
function catchMon(mon, map=-1) {
    mapCondition = always_true()
    if(map>-1) mapCondition = map() == map
    return
        mapCondition &&
        enemyPokemonId() == mon &&
        prev(battleResult()) == 0 &&
        trigger_when(battleResult() == BATT_CAUGHT)
}
function pokenewsActive(kind) {
    pokenews = range(1, n_pokenews)
    
    return
        any_of(pokenews, i =>
            pokenewsKind(i) == kind &&
            trigger_when(pokenewsState(i) == NEWS_STATE_ACTIVE)
        )
}
function partyMonDataStructGIsInSlot(slot, i=1) {
    valuesG = [0,1,2,3,4,5]
    if(slot==2) valuesG = [6,7,12,13,18,19]
    if(slot==3) valuesG = [8,10,14,16,20,22]
    if(slot==4) valuesG = [9,11,15,17,21,23]
    
    return __ornext(any_of(valuesG, v => partyMonPersonality(i)%24 == v))
}
function partyMonDataStructMIsInSlot(slot, i=1) {
    valuesM = [18,19,20,21,22,23]
    if(slot==2) valuesM = [0,1,14,15,20,21]
    if(slot==3) valuesM = [2,4,12,17,18,23]
    if(slot==4) valuesM = [3,5,13,16,19,22]
    
    return __ornext(any_of(valuesM, v => partyMonPersonality(i)%24 == v))
}
function partyMonDataStructAIsInSlot(slot, i=1) {
    valuesA = [6,7,8,9,10,11]
    if(slot==2) valuesA = [0,1,14,15,20,21]
    if(slot==3) valuesA = [2,4,12,17,18,23]
    if(slot==4) valuesA = [3,5,13,16,19,22]
    
    return __ornext(any_of(valuesA, v => partyMonPersonality(i)%24 == v))
}

// specific trigger functions
function buyCookie() {
    return
        pointerSanity() &&
        map() == MAP_MT_CHIMNEY &&
        (prev(money()) ^ securityKey()) - (money() ^ securityKey()) == 200
}
function reporters() {
    return
        pointerSanity() &&
        flag(TRAINER_GABBY_AND_TY_6) == prev(flag(TRAINER_GABBY_AND_TY_6)) &&
        flag(TRAINER_GABBY_AND_TY_6) == 1 &&
        counter(o_reportersBattles) > prev(counter(o_reportersBattles))
}
function sizeContest() {
    return
        pointerSanity() &&
        map() == MAP_SOOTOPOLIS_CITY_LOTAD_AND_SEEDOT_HOUSE &&
        prev(counter(o_sizeLotad)) >= 0x8000 &&
        prev(counter(o_sizeSeedot)) >= 0x8000 &&
        (
            (
                counter(o_sizeLotad) > prev(counter(o_sizeLotad)) && 
                counter(o_sizeSeedot) > 0x8000 
            ) || 
            (
                counter(o_sizeSeedot) > prev(counter(o_sizeSeedot)) && 
                counter(o_sizeLotad) > 0x8000
            )
        )
}
function betBf() {
    return
        pointerSanity() &&
        //map() == MAP_BATTLE_FRONTIER_LOUNGE1 && // TODO: check which house
        counter(o_gamblerBfAmmount) == BET_AMMOUNT_15 &&
        prev(counter(o_gamblerBfState)) == BET_STATE_WON &&
        counter(o_gamblerBfState) == 0
}
function clearance() {
    return
        pointerSanity() &&
        pokenewsActive(NEWS_KIND_CLEARANCE) &&
        trigger_when(prev(map()) == MAP_LILYCOVE_CITY_DEPARTMENT_STORE_5F) &&
        trigger_when(map() == MAP_LILYCOVE_CITY_DEPARTMENT_STORE_ROOFTOP)
}
function gameCorner() {
    return
        pointerSanity() &&
        pokenewsActive(NEWS_KIND_GAMECORNER) &&
        trigger_when(map() == MAP_MAUVILLE_CITY_GAME_CORNER) &&
        trigger_when(slotsPayout() == SLOT_PAYOUT_BIGBONUS) &&
        trigger_when(prev(slotsState()) == SLOT_STATE_STOP_REEL) &&
        trigger_when(slotsState() == SLOT_STATE_WIN)
}
function blendmaster() {
    return
        pointerSanity() &&
        pokenewsActive(NEWS_KIND_BLENDMASTER) &&
        trigger_when(map() == MAP_LILYCOVE_CITY_CONTEST_LOBBY) &&
        trigger_when(blendPlayers() == 2) &&
        trigger_when(prev(blendStateEnd()) == BLEND_STATE_FINISHED) &&
        trigger_when(blendStateEnd() == BLEND_STATE_END)
}
function scottReward() {
    return
        map() == MAP_BATTLE_FRONTIER_SCOTTS_HOUSE &&
        counter(o_scottConversations) == SCOTT_CONVOS &&
        flagTrigger(FLAG_SCOTT_GIVES_BATTLE_POINTS)
}
function effortRibbon() {
    return
        map() == MAP_SLATEPORT_CITY &&
        partyMonPersonality() == prev(partyMonPersonality()) &&
        any_of(range(1,n_pokemonDataStructs), struct =>
            partyMonDataStructMIsInSlot(struct) &&
            partyMonRibbons(struct) & RIBBON_EFFORT != prev(partyMonRibbons(struct)) & RIBBON_EFFORT
        )
}
function trainApprentice() {
    return
        pointerSanity(2) &&
        map() == MAP_BATTLE_FRONTIER_BATTLE_TOWER_LOBBY &&
        playerApprenticeData() < prev(playerApprenticeData()) && //settings wont go down in normal conditions, so it's safe to assume it's a reset
        any_of(range(1, n_apprentices), a =>
            prev(playerApprenticeId()) == apprenticeId(a) &&
            apprenticeTID(a) == playerTID()
        )
}
function defeatApprentice() {
    return
        pointerSanity(2) &&
        trainerId() >= 400 &&
        trainerId() < 404 &&
        apprenticeTID(trainerId()-400) == playerTID() &&
        prev(battleResult()) == 0 &&
        trigger_when(battleResult() == BATT_WON)
}
function partnerApprentice() {
    return
        pointerSanity(2) &&
        prev(apprenticeTID(bfTrainer(BF_TRAINER_APPRENTICE)-400)) == playerTID() &&
        prev(bfTrainer(BF_TRAINER_APPRENTICE)) >= 400 &&
        prev(bfTrainer(BF_TRAINER_APPRENTICE)) < 404 &&
        trigger_when(bfTrainer(BF_TRAINER_PARTNER) == prev(bfTrainer(BF_TRAINER_APPRENTICE))) &&
        trigger_when(bfTrainer(BF_TRAINER_PARTNER) != prev(bfTrainer(BF_TRAINER_PARTNER)))
}
function nuggetTrickHouse() {
    return
        pointerSanity() &&
        map() == MAP_ROUTE110_TRICK_HOUSE_END &&
        counter(o_trickHouseLevel) == 8 &&
        trigger_when(flag(FLAG_HIDDEN_ITEM_TRICK_HOUSE_NUGGET) > prev(flag(FLAG_HIDDEN_ITEM_TRICK_HOUSE_NUGGET)))
}
function stealItem(rematch) {
    item = rematch[REMATCH_ITEM]
    return
        isInBattle() &&
        trainerId() == rematch[REMATCH_TRAINER] &&
        enemyPokemonId() == rematch[REMATCH_MON] &&
        trigger_when(prior(enemyPokemonHeldItem()) == item) &&
        trigger_when(myPokemonHeldItem() == item) &&
        trigger_when(prev(myPokemonHeldItem()) != myPokemonHeldItem())
}
// bullshit function that only makes the right alts. Change addsource of XOR by remember, then get the right offset and XOR by recall <=50
function receiveFrustration() {
    return
        pointerSanity() &&
        flag(FLAG_RECEIVED_FANCLUB_TM_THIS_WEEK) > prev(flag(FLAG_RECEIVED_FANCLUB_TM_THIS_WEEK)) &&
        any_of(range(1,n_pokemonDataStructs), struct =>
            partyMonDataStructGIsInSlot(struct) &&
            partyMonFriendship(struct) ^ (partyMonPersonality(size=1, offset=1) ^ partyMonTID(size=1, offset=1)) <= 50
        )
}
// the measured is VERY irritating, so removing it
function fillDecos(total) {
    sumDecos = sum_of(range(1, n_roomDecos), i => roomDeco(i) / roomDeco(i))
    if(total==n_baseDecos) sumDecos = sum_of(range(1, n_baseDecos), i => baseDeco(i) / baseDeco(i))
    return
        pointerSanity() &&
        sumDecos == total && 
        prev(sumDecos) == total-1
}
function allDecos(decos, tents=false) {
    // map the deco list to a list of conditions for inventory check. 
    // If it includes the tents, add the check for finishing the trick house, since both tents are excluded
    // We do this to have a full condition list for the tally function
    conditions = array_map(decos, deco => __ornext(any_of(range(0, decorSlots[decos[deco]]), i => decorId(i, decos[deco])==deco)))
    if(tents) array_push(conditions, counter(o_trickHouseLevel)==8)
    total = length(conditions)
    // get full deco inventory. Easiest to figure what types are on the array ig. Type doll is the last one on the list
    totalDecoSlots = 0
    for j in range(0, DECOR_TYPE_DOLL) totalDecoSlots = totalDecoSlots + decorSlots[j]
    
    return
        pointerSanity() &&
        measured(tally_of(conditions, total, c => once(c))) &&
        // delta check for any deco increasing. Type desk is the first one on the list
        any_of(range(0, totalDecoSlots), k => prev(decorId(k, DECOR_TYPE_DESK)) == 0 && decorId(k, DECOR_TYPE_DESK) != 0)
}
function pichu() {
    return
        any_of(range(1,6), slot =>
            never(partyMonPersonality(slot) != prev(partyMonPersonality(slot))) &&
            once(partyMonIsEgg(slot) == 1) &&
            partyMonIsEgg(slot) == 0 &&
            any_of(range(1,n_pokemonDataStructs), struct =>
                partyMonDataStructAIsInSlot(struct, slot) && // adds the personality % 24 checks to an alt for a struct
                any_of(range(1,4), move => partyMonMoveId(struct, slot, move) ^ (partyMonPersonality(slot, size=2, offset=(((move-1)%2)*2)) ^ partyMonTID(slot, size=2, offset=(((move-1)%2)*2))) == MOVE_ID_VOLTTACKLE) 
            )
        ) || 
        ( 
            enemyPokemonId()==SPECIES_PIKACHU &&
            enemyPokemonHeldItem()==ITEM_LIGHT_BALL &&
            prev(battleResult())==0 &&
            trigger_when(always_false())
        )
}
function winBattleTent(tent) {
    return
        pointerSanity(2) &&
        map() == tent &&
        prev(battleChallengeStatus()) == 3 &&
        battleChallengeStatus() == 0
}
function flutes() {
    bagItems = range(1, n_bagItems)
    return
        pointerSanity() &&
        once(counter(o_ashCount) < prev(counter(o_ashCount))) &&
        measured(tally_of(listFlutes, length(listFlutes), flute =>
            once(__ornext(any_of(bagItems, i => bagItem(i) == flute)))
        )) &&
        any_of(bagItems, i => bagItem(i)!=prev(bagItem(i)))
        
}
function shellBell() {
    return
        (map()==MAP_SHOAL_CAVE_HIGH_TIDE_ENTRANCE_ROOM || map()==MAP_SHOAL_CAVE_LOW_TIDE_ENTRANCE_ROOM) &&
        flagTrigger(FLAG_TEMP_2)
}
function trainerHillBeat(mode) {
    return
        map() == MAP_TRAINER_HILL_ROOF &&
        trainerHillMode() == (mode*0x40) &&
        trainerHillEnd() > prev(trainerHillEnd())
}
// doesn't work, probably doesn't get set before the pointer change like flags
function winMasterContestFAIL(type) {
    return
        pointerSanity() &&
        contestWinnerTID() == playerTID() &&
        contestWinnerRank() == CON_RANK_MASTER &&
        contestWinnerType() == type &&
        // we check that there is a change in the mon that won, or contest attempted. 
        // Would still fail if the same mon won the same contest 2 times in a row, but who would do that
        (
            contestWinnerPV() != prev(contestWinnerPV()) ||
            contestWinnerRank() != prev(contestWinnerRank()) ||
            contestWinnerType() != prev(contestWinnerType()) 
        )
}
function hyperCut() => cutRadio()==5 && prev(cutRadio())<5
function fullDex() {
    alts = always_false()
    total = length(dexNoChoices) + 5 
    for starter in [NATIONAL_DEX_TREECKO, NATIONAL_DEX_TORCHIC, NATIONAL_DEX_MUDKIP] {
        dexAlt = []
        array_push(dexAlt, dexFlag(starter))
        array_push(dexAlt, dexFlag(starter+1))
        array_push(dexAlt, dexFlag(starter+2))
        moons = dexFlag(NATIONAL_DEX_WIGGLYTUFF) + dexFlag(NATIONAL_DEX_DELCATTY) - (dexFlag(NATIONAL_DEX_WIGGLYTUFF) * dexFlag(NATIONAL_DEX_DELCATTY))
        array_push(dexAlt, moons)
        latis = dexFlag(NATIONAL_DEX_LATIAS) + dexFlag(NATIONAL_DEX_LATIOS) - (dexFlag(NATIONAL_DEX_LATIAS) * dexFlag(NATIONAL_DEX_LATIOS))
        array_push(dexAlt, latis)
        for i in dexNoChoices array_push(dexAlt, dexFlag(i))
        sum = sum_of(dexAlt, n => n)
        
        alts = alts || (measured(sum==total) && prev(sum)==total-1) // fuck whoever lets Nincada for the last
    }
    
    return pointerSanity(2) && alts
}
function rematchFinal(flag) {
    return
        prev(rematch(rematches[flag])) > 0 &&
        trigger_when(flagTrigger(flag))
}


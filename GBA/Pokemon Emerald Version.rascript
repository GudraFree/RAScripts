function leebo(leeb) {
    leaderboard(
        leeb[LB_TITLE],
        leeb[LB_DESC],
        isPaused() && input(leeb[LB_INP_1])==1 && input(leeb[LB_INP_2])==1 && prev(input(leeb[LB_INP_2]))==0,
        always_false(),
        always_true(),
        gameStat(leeb[LB_VALUE]) ^ securityKey()
    )
}

